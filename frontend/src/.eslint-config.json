{
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "plugins": ["eslint-plugin-eslint-comments", "complexity"],
    "rules": {
      "eslint-comments/no-unused-disable": "error",
      "complexity": ["error", 10, "warn", 5],
      "semi": ["error", "always"], 
      "no-unused-vars": "error",
      "camelcase": "error", 
      "no-undef": "error", 
      "consistent-return": "error", 
      "no-nested-ternary": "error"
    }
  }
  